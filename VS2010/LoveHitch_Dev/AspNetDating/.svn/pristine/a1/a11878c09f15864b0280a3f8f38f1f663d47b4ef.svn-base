                        }
                    });
                }
                else if (tipContentString != null && tipContentString  != "") {
                    var selector = $("#showTipPopup");
                    $("#showTipPopup").html(unescape(tipContentString));
                    $("#showTipPopup").attr('title', tipTitleString);
